<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Protect Ster CRM - Login</title>

    
    {% stylesheets  filter='cssrewrite'      
             'bundles/pages/css/bootstrap/css/bootstrap.min.css'    
             'bundles/pages/css/metisMenu/dist/metisMenu.min.css' 
             'bundles/pages/css/font-awesome/css/font-awesome.min.css'        
             'bundles/pages/css/sb-admin-2.css' 
             'bundles/user/css/app.css' 

        %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
  <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

</head>

<body>

    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <div class="login-panel panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">{% trans %}Please Sign In{% endtrans %}</h3>
                    </div>
                    <div class="panel-body">
                      
                        
                          {% block fos_user_content %}
                         {% endblock fos_user_content %}
                    </div>
                </div>
            </div>
        </div>
    </div>

{% javascripts 
        'bundles/pages/js/jquery-1.11.3.min.js' 
        'bundles/pages/css/bootstrap/js/bootstrap.min.js' 
        'bundles/pages/js/sb-admin-2.js' 
        'bundles/pages/css/metisMenu/dist/metisMenu.min.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
</body>

</html>